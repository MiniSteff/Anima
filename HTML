        if (constitution == 10)  { setAttrs({ zeon_base2: 135 })}
        if (constitution == 11)  { setAttrs({ zeon_base2: 150 })}
        if (constitution == 12)  { setAttrs({ zeon_base2: 160 })}
        if (constitution == 13)  { setAttrs({ zeon_base2: 175 })}
        if (constitution == 14)  { setAttrs({ zeon_base2: 185 })}
        if (constitution == 15)  { setAttrs({ zeon_base2: 200 })}
        if (constitution == 16)  { setAttrs({ zeon_base2: 215 })}
        if (constitution == 17)  { setAttrs({ zeon_base2: 225 })}
        if (constitution == 18)  { setAttrs({ zeon_base2: 240 })}
        if (constitution == 19)  { setAttrs({ zeon_base2: 250 })}
        if (constitution == 20)  { setAttrs({ zeon_base2: 265 })};
    })});
    
    on("change:pouvoir_actuel", function() { getAttrs(["pouvoir_actuel","amr_base2"], function(values) {
        var constitution = values.pouvoir_actuel;
        var pv           = values.amr_base2;
        
        if (constitution == 1)   { setAttrs({ amr_base2: 0  })}
        if (constitution == 2)   { setAttrs({ amr_base2: 0  })}
        if (constitution == 3)   { setAttrs({ amr_base2: 0  })}
        if (constitution == 4)   { setAttrs({ amr_base2: 0  })}
        if (constitution == 5)   { setAttrs({ amr_base2: 5  })}
        if (constitution == 6)   { setAttrs({ amr_base2: 5  })}
        if (constitution == 7)   { setAttrs({ amr_base2: 5  })}
        if (constitution == 8)   { setAttrs({ amr_base2: 10 })}
        if (constitution == 9)   { setAttrs({ amr_base2: 10 })}
        if (constitution == 10)  { setAttrs({ amr_base2: 10 })}
        if (constitution == 11)  { setAttrs({ amr_base2: 10 })}
        if (constitution == 12)  { setAttrs({ amr_base2: 15 })}
        if (constitution == 13)  { setAttrs({ amr_base2: 15 })}
        if (constitution == 14)  { setAttrs({ amr_base2: 15 })}
        if (constitution == 15)  { setAttrs({ amr_base2: 20 })}
        if (constitution == 16)  { setAttrs({ amr_base2: 25 })}
        if (constitution == 17)  { setAttrs({ amr_base2: 25 })}
        if (constitution == 18)  { setAttrs({ amr_base2: 30 })}
        if (constitution == 19)  { setAttrs({ amr_base2: 30 })}
        if (constitution == 20)  { setAttrs({ amr_base2: 35 })};
    })});
    
    on("change:force_actuel", function() { getAttrs(["force_actuel","for_accumulation_par_tour2"], function(values) {
        var attribut = values.force_actuel;
        
        if (attribut <= 9)                          { setAttrs({ for_accumulation_par_tour2: 1  })}
        if ((attribut >= 10) && (attribut <= 12))   { setAttrs({ for_accumulation_par_tour2: 2  })}
        if ((attribut >= 13) && (attribut <= 15))   { setAttrs({ for_accumulation_par_tour2: 3  })}
        if (attribut >= 16)                         { setAttrs({ for_accumulation_par_tour2: 4  })};
    })});
    
    on("change:dexterite_actuel", function() { getAttrs(["dexterite_actuel","dex_accumulation_par_tour2"], function(values) {
        var attribut = values.dexterite_actuel;
        
        if (attribut <= 9)                          { setAttrs({ dex_accumulation_par_tour2: 1  })}
        if ((attribut >= 10) && (attribut <= 12))   { setAttrs({ dex_accumulation_par_tour2: 2  })}
        if ((attribut >= 13) && (attribut <= 15))   { setAttrs({ dex_accumulation_par_tour2: 3  })}
        if (attribut >= 16)                         { setAttrs({ dex_accumulation_par_tour2: 4  })};
    })});
    
    on("change:agilite_actuel", function() { getAttrs(["agilite_actuel","agi_accumulation_par_tour2"], function(values) {
        var attribut = values.agilite_actuel;
        
        if (attribut <= 9)                          { setAttrs({ agi_accumulation_par_tour2: 1  })}
        if ((attribut >= 10) && (attribut <= 12))   { setAttrs({ agi_accumulation_par_tour2: 2  })}
        if ((attribut >= 13) && (attribut <= 15))   { setAttrs({ agi_accumulation_par_tour2: 3  })}
        if (attribut >= 16)                         { setAttrs({ agi_accumulation_par_tour2: 4  })};
    })});
    
    on("change:constitution_actuel", function() { getAttrs(["constitution_actuel","con_accumulation_par_tour2"], function(values) {
        var attribut = values.constitution_actuel;
        
        if (attribut <= 9)                          { setAttrs({ con_accumulation_par_tour2: 1  })}
        if ((attribut >= 10) && (attribut <= 12))   { setAttrs({ con_accumulation_par_tour2: 2  })}
        if ((attribut >= 13) && (attribut <= 15))   { setAttrs({ con_accumulation_par_tour2: 3  })}
        if (attribut >= 16)                         { setAttrs({ con_accumulation_par_tour2: 4  })};
    })});
    
    on("change:pouvoir_actuel", function() { getAttrs(["pouvoir_actuel","pou_accumulation_par_tour2"], function(values) {
        var attribut = values.pouvoir_actuel;
        
        if (attribut <= 9)                          { setAttrs({ pou_accumulation_par_tour2: 1  })}
        if ((attribut >= 10) && (attribut <= 12))   { setAttrs({ pou_accumulation_par_tour2: 2  })}
        if ((attribut >= 13) && (attribut <= 15))   { setAttrs({ pou_accumulation_par_tour2: 3  })}
        if (attribut >= 16)                         { setAttrs({ pou_accumulation_par_tour2: 4  })};
    })});
    
    on("change:volonte_actuel", function() { getAttrs(["volonte_actuel","vol_accumulation_par_tour2"], function(values) {
        var attribut = values.volonte_actuel;
        
        if (attribut <= 9)                          { setAttrs({ vol_accumulation_par_tour2: 1  })}
        if ((attribut >= 10) && (attribut <= 12))   { setAttrs({ vol_accumulation_par_tour2: 2  })}
        if ((attribut >= 13) && (attribut <= 15))   { setAttrs({ vol_accumulation_par_tour2: 3  })}
        if (attribut >= 16)                         { setAttrs({ vol_accumulation_par_tour2: 4  })};
    })});
    
    
</script>
