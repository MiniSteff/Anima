<!-- SCRIPT ________________________________________________________________ -->

<script type="text/worker">
    on("change:add_accumulation", function() { getAttrs(["add_accumulation", "for_accumulation_actuel","dex_accumulation_actuel", "agi_accumulation_actuel", "con_accumulation_actuel", "pou_accumulation_actuel", "vol_accumulation_actuel", "for_accumulation_par_tour2","dex_accumulation_par_tour2", "agi_accumulation_par_tour2", "con_accumulation_par_tour2", "pou_accumulation_par_tour2", "vol_accumulation_par_tour2","for_accumulation_bonus", "dex_accumulation_bonus", "agi_accumulation_bonus", "con_accumulation_bonus", "pou_accumulation_bonus", "vol_accumulation_bonus",], function(values) {
    if(values.add_accumulation == 0){
        var force        = Number(values.for_accumulation_actuel)+Number(values.for_accumulation_par_tour2)+Number(values.for_accumulation_bonus);
        var dexterite    = Number(values.dex_accumulation_actuel)+Number(values.dex_accumulation_par_tour2)+Number(values.dex_accumulation_bonus);
        var agilite      = Number(values.agi_accumulation_actuel)+Number(values.agi_accumulation_par_tour2)+Number(values.agi_accumulation_bonus);
        var constitution = Number(values.con_accumulation_actuel)+Number(values.con_accumulation_par_tour2)+Number(values.con_accumulation_bonus);
        var pouvoir      = Number(values.pou_accumulation_actuel)+Number(values.pou_accumulation_par_tour2)+Number(values.pou_accumulation_bonus);
        var volonte      = Number(values.vol_accumulation_actuel)+Number(values.vol_accumulation_par_tour2)+Number(values.vol_accumulation_bonus);
        
        setAttrs({ add_accumulation:        1});
        setAttrs({ for_accumulation_actuel: force});
        setAttrs({ dex_accumulation_actuel: dexterite});
        setAttrs({ agi_accumulation_actuel: agilite});
        setAttrs({ con_accumulation_actuel: constitution});
        setAttrs({ pou_accumulation_actuel: pouvoir});
        setAttrs({ vol_accumulation_actuel: volonte});
    }
    else{}
    
    })});

